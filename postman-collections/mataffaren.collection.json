		{
			"name": "Banan inte finns i fel kategorie",
			"event": [
				{
					"listen": "test",
					"script": {
						"exec": [
							"// Parse the response to get the product list\r",
							"pm.test(\"Status code is 200\", function () {\r",
							"    pm.response.to.have.status(200);\r",
							"});\r",
							"\r",
							"pm.test(\"Response time is less than 1 second\", function () {\r",
							"    pm.expect(pm.response.responseTime).to.be.below(1000);\r",
							"});\r",
							"\r",
							"function checkSubcategories(categories, baseUrl, excludeUrl, completedRequests, totalRequests) {\r",
							"    categories.forEach(category => {\r",
							"        if (category.url === excludeUrl) return;\r",
							"\r",
							"        const requestUrl = `${baseUrl}${category.url}`;\r",
							"        pm.sendRequest(requestUrl, (err, res) => {\r",
							"            if (!err) {\r",
							"                try {\r",
							"                    const products = res.json().results || [];\r",
							"                    const productExists = products.some(product => product.title === \"Banan Klass 1\");\r",
							"                    \r",
							"                    pm.test(`Produkten finns inte i kategori ${category.url}`, () => {\r",
							"                        pm.expect(productExists).to.be.false;\r",
							"                    });\r",
							"                } catch (e) {\r",
							"                }\r",
							"            }\r",
							"\r",
							"            completedRequests.count++;\r",
							"\r",
							"            if (completedRequests.count === totalRequests.count) {\r",
							"                pm.test(\"Alla kategorier och subkategorier kontrollerades utan fel\", () => {\r",
							"                    pm.expect(completedRequests.count).to.equal(totalRequests.count);\r",
							"                });\r",
							"            }\r",
							"        });\r",
							"\r",
							"        if (category.children && category.children.length > 0) {\r",
							"            checkSubcategories(category.children, baseUrl, excludeUrl, completedRequests, totalRequests);\r",
							"        }\r",
							"    });\r",
							"}\r",
							"pm.sendRequest(\"http://localhost:4000/api/leftMenu/categorytree\", (err, res) => {\r",
							"    if (err) return;\r",
							"\r",
							"    pm.test(\"Banan Klass 1 finns inte i andra kategorier och subkategorier\", () => {\r",
							"        const categoryTree = res.json();\r",
							"        const baseUrl = \"http://localhost:4000/api\";\r",
							"        const excludeUrl = \"/frukt-och-gront/frukt/bananer\";\r",
							"        const totalRequests = { count: 0 };\r",
							"        const completedRequests = { count: 0 };\r",
							"\r",
							"        function countCategories(categories) {\r",
							"            categories.forEach(category => {\r",
							"                if (category.url !== excludeUrl) {\r",
							"                    totalRequests.count++;\r",
							"                    if (category.children && category.children.length > 0) {\r",
							"                        countCategories(category.children);\r",
							"                    }\r",
							"                }\r",
							"            });\r",
							"        }\r",
							"\r",
							"        countCategories(categoryTree.children);\r",
							"        checkSubcategories(categoryTree.children, baseUrl, excludeUrl, completedRequests, totalRequests);\r",
							"    });\r",
							"});\r",
							""
